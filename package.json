{
  "name": "react-es6-webpack-eslint-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate code to work with ES6, Webpack and ESLint",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "lint": "esw --color --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty . *.js",
    "lint:watch": "npm run lint -- --watch",
    "clean": "rimraf ./dist && mkdir dist",
    "build": "webpack --config webpack.config.production.js",
    "build: watch": "webpack --config webpack.config.production.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hakanmhmd/react-es6-webpack-eslint-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "es6",
    "babel",
    "eslint",
    "reactjs"
  ],
  "author": "Hakan Mehmed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hakanmhmd/react-es6-webpack-eslint-boilerplate/issues"
  },
  "homepage": "https://github.com/hakanmhmd/react-es6-webpack-eslint-boilerplate#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.10.1",
    "open-browser-webpack-plugin": "0.0.5",
    "react-hot-loader": "^1.3.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "jquery": "^3.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.4.0"
  }
}
